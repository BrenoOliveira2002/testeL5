generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Produtos {
  id       Int     @id @default(autoincrement())
  nome     String
  categoria String
  preco     Int

  @@map(name: "produtos")
}

model Pessoas {
  id       Int     @id @default(autoincrement())
  nome     String
  idade    String
  endereco String

  @@map(name: "pessoas")
}

model Pagamentos {
  id       Int     @id @default(autoincrement())
  description String
  
  @@map(name: "pagamentos")
}
